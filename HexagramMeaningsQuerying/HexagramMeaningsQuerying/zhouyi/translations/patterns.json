  "ApplySixes": "用六：{1}{0}",

  // The pattern to be used when calling a yin line's ToString(),
  //
  // {0}: A new line. This depends on the running environment.
  // {1}: Text of the line.
  "YinLines": [
    "初六：{1}",
    "六二：{1}",
    "六三：{1}",
    "六四：{1}",
    "六五：{1}",
    "上六：{1}"
  ],

  // The pattern to be used when calling a yang line's ToString(),
